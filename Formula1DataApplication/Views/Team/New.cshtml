
@{
    ViewBag.Title = "New";
}

<h2>Create Team</h2>
<form action="/Team/Create" method="POST">
    <div>
        <label>Name:</label> <input type="text" name="TeamName" />
    </div>
    <div>
        <label>Country:</label>
        <input type="text" name="TeamCountry" />
    </div>
    <div>
        <label>Year:</label> <input type="number" name="TeamYear" />
    </div>
    <div>
        <input type="submit" value="Create Team" />

    </div>
</form>
